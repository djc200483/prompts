{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "generator.html",
      "use": "@vercel/static"
    },
    {
      "src": "blog.html",
      "use": "@vercel/static"
    },
    {
      "src": "terms.html",
      "use": "@vercel/static"
    },
    {
      "src": "blog-post.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.png",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/generator",
      "destination": "/generator.html"
    },
    {
      "source": "/generator/",
      "destination": "/generator.html"
    },
    {
      "source": "/blog",
      "destination": "/blog.html"
    },
    {
      "source": "/blog/",
      "destination": "/blog.html"
    },
    {
      "source": "/blog/(.*)",
      "destination": "/blog-post.html"
    },
    {
      "source": "/terms",
      "destination": "/terms.html"
    },
    {
      "source": "/terms/",
      "destination": "/terms.html"
    },
    {
      "source": "/((?!.*\\.png$|^generator$|^blog$|^terms$).*)",
      "destination": "/index.html"
    }
  ],
  "cleanUrls": false
}
